generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Survey {
  id Int @id @default(autoincrement())
  name String

  questions Question[]
}

model Question {
  id Int @id @default(autoincrement())
  index Int
  surveyId Int
  description String

  survey Survey @relation(fields: [surveyId], references: [id])
}